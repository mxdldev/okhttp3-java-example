        />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="用户注册1"
        android:onClick="addUser1"
        />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="用户登陆2"
